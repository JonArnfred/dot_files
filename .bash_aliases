# cd easily
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias ....="cd ../../../../"

# protect yourself
alias rm='rm -i'
alias mv='mv -i'
alias cp='cp -i'

# ls more default info
alias ll='ls -laF'
alias ls='ls -F'


# vim
alias v="vim"

# python
alias p="python3"

# make
alias m="make"
alias mc="make clean"

# aliases available when sudo
alias sudo="sudo "
